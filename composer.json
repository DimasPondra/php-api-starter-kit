{
    "name": "pondra/php-api-starter-kit",
    "description": "PHP API Starter Kit",
    "type": "project",
    "authors": [
        {
            "name": "dimas pondra",
            "email": "dimpullndra@gmail.com"
        }
    ],
    "require": {
        "php": ">=8",
        "vlucas/phpdotenv": "^5.6",
        "ramsey/uuid": "^4.7",
        "phpmailer/phpmailer": "^6.10",
        "monolog/monolog": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.7"
    },
    "autoload": {
        "psr-4": {
            "Pondra\\PhpApiStarterKit\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pondra\\PhpApiStarterKit\\": "tests/"
        }
    }
}
